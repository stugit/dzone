    <div class="col-xs-12 icon-container">

        <div class="col-xs-12">
            <img src="/themes/dz20/images/DZone_MoleculeGradient.png" ng-class="{'top-d-image':isDownload}">
            <p ng-if="!isDownload">Trusted by more than one million developers worldwide</p>
        </div>

        <div ng-if="isDownload" class="col-xs-4">
            <img ng-src="{{asset.image}}" class="pull-right download-img">
        </div>
        <div ng-if="isDownload" class="col-xs-8 col-sm-6">
            <p class="p-download">Log In or Sign Up to continue to your download!</p>
        </div>
    </div>

    <div class="col-xs-12 box-container" ng-cloak>
        <div class="col-xs-0 col-sm-0 col-md-2 col-lg-2 col-xl-2 corners"></div>
        <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 col-xl-4 black-container">
            <form role="form" method="POST" action="/j_spring_security_check" ng-if="option == 0">
                <h2>Already a Member?</h2>
                <input type="hidden" name="TH_CSRF" value="-5031570936583979578"/>
                <input name="_spring_security_remember_me" type="hidden" value="true" />
                <div class="col-sm-1 col-xs-2 icons first-icon">
                    <span class="icon-mail-alt"></span>
                </div>
                <div class="form-group col-sm-11 col-xs-10 first-input" ng-class="{'has-error': error}">
                    <input type="text" name="j_username" autocomplete="off" ng-model="credentials.username" class="my-form-control" placeholder="Email Address or Username" />
                </div>
                <div class="col-sm-1 icons col-xs-2">
                    <span class="icon-key"></span>
                </div>
                <div class="form-group col-sm-11 col-xs-10" ng-class="{'has-error': error}">
                    <input type="password" name="j_password" ng-model="credentials.password" class="my-form-control" placeholder="Password" />
                    <p ng-if="error" class="error-p">Incorrect Username or Password</p>

                </div>
                <div class="col-sm-1 col-xs-2 icons">
                </div>
                <div class="form-group button-right col-sm-11 col-xs-10 member-button">
                    <button type="submit" ng-disabled="!credentials.username || !credentials.password" class="btn">Log In</button>
                </div>
                <div class="forgot-pass col-xs-12">
                    <a ng-href="" ng-click="setStep(1)" >Forgot Password?</a>
                </div>
                <div class="not-member col-xs-12">
                    <a ng-href="" ng-click="toggleView()" >Not a member?</a>
                </div>
            </form>

            <div ng-if="option == 1">
                <h2 class="h2-password">Forgot your password?</h2>
                <p class="p-password">Enter the email address you gave us when you signed up and we will send you a link to create a new password</p>
                <div class="form-group input-container first-input" ng-class="{'has-error': cantRecover}">
                    <input type="email" ng-model="credentials.username" name="email" autocomplete="off" class="my-form-control" placeholder="Enter your Email">
                    <!--<div class="icon" ng-class="{'ok_icon fa fa-check': !userEmailForm.email.$invalid && user.email != ''}"></div>-->
                    <span class="text-warning" ng-if="cantRecover">We couldn't find your email</span>
                </div>
                <div class="form-group">
                    <div class="member-button button-right">
                        <button ng-click="requestPassword()" ng-disabled="!credentials.username || credentials.username.length < 2" type="submit" class="btn btn-success btn-reset-link" value="SEND MY RESET LINK">Send My Reset Link</button>
                    </div>
                </div>
                <div>
                    <div class="forgot-pass">
                        <a ng-href="" ng-click="setStep(0)" >Return to login</a>
                    </div>
                </div>
            </div>

            <div ng-if="option == 2">
                <h2 class="h2-password">Success!</h2>
                <p class="p-password">Email sent to <span> {{ credentials.username }}</span>.
                Click the link in the email to reset your password. </p>
                <p class="no-email-title">No email after 15 minutes?</p>
                <p class="p-password">Check your spam folder for an email from <span class="no-reply"> noreply@dzone.com</span></p>
                <p class="still-no-email" >Still no email? Contact support: <a ng-href="mailto:support@dzone.com">support@dzone.com</a></p>
                <div class="forgot-pass">
                    <a ng-href="" ng-click="setStep(0)" >Return to login</a>
                </div>

            </div>
        </div>

        <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 col-xl-4 white-container">
            <h2>Join DZone for Free!</h2>
            <p>Get updates, free resources, and the opportunity to share your knowledge in a professional community.</p>
            <input name="_spring_security_remember_me" type="hidden" value="true" />
            <form method="post" action="/user/register" name="emailForm">
                <input type="hidden" name="TH_CSRF" value="{{ $root.csrfToken }}" />
                <div class="col-md-12 member-button first-input">
                    <input type="email" ng-model="registration.email" name="email" autocomplete="off" class="my-form-control" placeholder="Enter email address">
                    <div class="icon" ng-class="{'icon-ok': (!emailForm.email.$invalid && registration.email !='' && registration.email !=null) }"></div>
                </div>
                <div class="col-md-12 member-button">
                    <button class="btn" ng-disabled="emailForm.email.$invalid || !registration.email !=' '" type="submit">Sign Up</button>
                </div>
                <div class="not-member already-member col-xs-12">
                    <a ng-href="" ng-click="toggleView()" >Already a member?</a>
                </div>
            </form>
        </div>

        <div class="col-xs-0 col-sm-0 col-md-2 col-lg-2 col-xl-2 corners"></div>
    </div>
</div></div></div></div>

